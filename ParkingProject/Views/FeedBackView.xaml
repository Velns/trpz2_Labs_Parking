<catel1:UserControl
    x:Class="Parking.Views.FeedBackView"
    xmlns:catel1="http://catel.codeplex.com"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:catel="http://schemas.catelproject.com">

    <Grid>

        <Label Content="FeedBack" HorizontalAlignment="Center" Margin="0,5,0,0" VerticalAlignment="Top" FontSize="24" FontStyle="Italic" FontWeight="Bold" FontFamily="Times New Roman"/>
        <TextBox Height="25" Margin="5,45,5,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" VerticalContentAlignment="Center"/>
        <TextBox Margin="5,75,5,35" TextWrapping="Wrap" Text="TextBox"/>
        <Button Content="Send" HorizontalAlignment="Right" Margin="0,0,5,5" VerticalAlignment="Bottom" Width="80" Height="20"/>

    </Grid>

</catel1:UserControl>
