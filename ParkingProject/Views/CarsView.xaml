<catel1:UserControl
    x:Class="Parking.Views.CarsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:catel="http://schemas.catelproject.com"
    xmlns:catel1="http://catel.codeplex.com">

    <Grid>
        <ListView
            Height="100"
            Margin="0,39,0,0"
            VerticalAlignment="Top"
            ItemsSource="{Binding userList}">
            <ListView.View>
                <GridView>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=Name}">Name</GridViewColumn>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=PhoneNum}">PhoneNum</GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>

</catel1:UserControl>
